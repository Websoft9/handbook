{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^\\.\\./"
    },
    {
      "pattern": "^\\.\/"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 2,
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308, 0],
  "httpHeaders": [
    {
      "urls": ["https://"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      }
    }
  ]
}
